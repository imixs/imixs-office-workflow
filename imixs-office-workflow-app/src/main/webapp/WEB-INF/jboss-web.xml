<?xml version="1.0" encoding="UTF-8"?>
<jboss-web>
	<!-- Mail Ref -->
	<resource-ref>
		<res-ref-name>mail/org.imixs.workflow.mail</res-ref-name>
		<res-type>jakarta.mail.Session</res-type>
		<jndi-name>${imixs-mail-res-ref-name}</jndi-name>
