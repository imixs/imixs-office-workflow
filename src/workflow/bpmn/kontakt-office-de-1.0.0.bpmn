          <imixs:value><![CDATA[ var isValid=true;
 var errorMessage="";
 // Datuml validierung
 if (!workitem.get('datdate')) {
     isValid=false;     
     errorMessage='Bitte geben Sie ein Wiedervorlagedatum ein.';
 }

 // E-Mail validierung
 if (!validateEmail("_email")) {
     isValid=false;     
     errorMessage='Bitte geben Sie eine gültige E-Mail Adresse ein';
 }


function validateEmail(fieldName) {
    var email = workitem.get(fieldName);
    if (email == null ) 
	return false;     
	
    var re = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
    return re.test(email[0]);
}]]></imixs:value>
